<launch>
	<node
		pkg="optoforce_etherdaq_driver"
		type="etherdaq_node"
		name="etherdaq_node"
		args="--address 192.168.1.105 --rate 100 --filter 4"
	/>
	<node
		pkg="optoforce_etherdaq_driver"
		type="etherdaq_subscriber"
		name="etherdaq_subscriber"
		output="screen"
	/>
    <node pkg="wrench_filter" type="mean_filter_node" name="mean_filter_node"/>
</launch>
